        "types": "./dist/bundle.d.mts",
        "default": "./dist/bundle.mjs"
      }
    }
  },
  "devDependencies": {
    "@types/bun": "latest",
    "oxfmt": "^0.18.0",
    "oxlint": "^1.33.0",
    "oxlint-tsgolint": "^0.9.1",
    "tsdown": "^0.18.1",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "keyword": [
    "date",
    "time",
    "date format"
  ]
}
