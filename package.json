{
  "name": "shi-jian",
  "version": "0.0.0",
  "description": "The easiest way to work with dates in JavaScript and TypeScript.",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.cts",
  "unpkg": "dist/bundle.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
